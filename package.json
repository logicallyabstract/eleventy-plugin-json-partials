{
  "name": "eleventy-plugin-json-partials",
  "version": "0.0.0",
  "description": "For each @11ty index.html file generated, create a JSON partial file to use in SPA routing patterns",
  "main": "dist/index.js",
  "scripts": {
    "eslint": "eslint --ext .js,.ts \"src/\" \"*.js\"",
    "lint": "run-s eslint typecheck",
    "test": "mocha -r esbuild-runner/register \"src/**/*.test.ts\" --extension ts",
    "test:doesntwork": "mocha -r esbuild-runner/register \"src/**/*.test.ts\" --extension ts",
    "test:loader": "mocha --loader=./test-helpers/esbuild-loader.js \"src/**/*.test.ts\" --extension ts --experimental-import-meta-resolve",
    "typecheck": "tsc --noEmit"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/logicallyabstract/eleventy-plugin-json-partials.git"
  },
  "keywords": [
    "eleventy",
    "11ty",
    "plugin",
    "json"
  ],
  "author": "Logically Abstract <aj@logicallyabstract.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/logicallyabstract/eleventy-plugin-json-partials/issues"
  },
  "homepage": "https://github.com/logicallyabstract/eleventy-plugin-json-partials#readme",
  "dependencies": {
    "cheerio": "^1.0.0-rc.11"
  },
  "devDependencies": {
    "@types/chai": "^4.3.1",
    "@types/mocha": "^9.1.1",
    "@types/node": "^17.0.36",
    "@types/sinon": "^10.0.11",
    "@typescript-eslint/eslint-plugin": "^5.27.1",
    "chai": "^4.3.6",
    "esbuild-runner": "^2.2.1",
    "eslint": "^8.16.0",
    "gts": "^3.1.0",
    "mocha": "^10.0.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.6.2",
    "sinon": "^14.0.0",
    "testdouble": "^3.16.5",
    "ts-node": "^10.8.1",
    "typescript": "^4.7.2"
  }
}
